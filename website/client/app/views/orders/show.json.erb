{
  <%= render partial: "order_attributes", locals: {order: @order} %>,

  "address": {
    "is_business_address": <%= raw(@order.is_business_address.to_json) %>,
    "business_name": <%= raw(@order.business_name.to_json) %>,
    "street_address": <%= raw(@order.street_address.to_json) %>,
    "city": <%= raw(@order.city.to_json) %>,
    "state": <%= raw(@order.state.to_json) %>,
    "zip_code": <%= raw(@order.zip_code.to_json) %>,
    "note": <%= raw(@order.note.to_json) %>
  },

  "cart_entry": <%= render partial: "cart_entries/template_collection", locals: {cartEntryCollection: @cartEntries, cartItemStoreInfoHash: nil, storeHash: nil} %>
}