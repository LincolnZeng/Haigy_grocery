{
  "id": <%= raw(info.id.to_json) %>,
  "is_category_item": <%= raw(info.is_category_item.to_json) %>,
  "price": <%= raw(info.price.to_json) %>,
  "sale_price": <%= raw(info.sale_price.to_json) %>,
  "on_sale": <%= raw(info.on_sale.to_json) %>,
  "estimated_weight": <%= info.estimated_weight_of_each_in_lb.to_json %>,
