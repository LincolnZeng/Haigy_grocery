  "store_name": <%= raw(info.store.company.name.to_json) %>,
  "store_address": <%= raw(info.store.formatted_address.to_json) %>,

  "item_id": <%= raw(info.item.id.to_json) %>,
  "item_name": <%= raw(info.item.name.to_json) %>,
  "item_cover_image_path": <%= raw(info.item.cover_image.url(:medium).to_json) %>,
